<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace：命名空间，用于隔离sql，namespace中的包名要和Dao/mapper 接口的包名一致！ -->
<mapper namespace="org.studyspringmybatis.demo.mybatis.mapper.UserMapper">


	<select id="findAll" resultType="org.studyspringmybatis.demo.mybatis.entity.User">
		select * from tbl_user
	</select>



</mapper>